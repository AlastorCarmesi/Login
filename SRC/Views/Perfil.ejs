<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Profile Page</title>
    <%- include ('./Style/head') %>
  </head>
  <body>
    <div class="container">
      <header class="text-center">
        <h1>Perfil</h1>
        <a href="/logout" class="btn btn-light btn-sm">
          Logout
        </a>
      </header>
      <hr>
      <div class="container">
        <div class="col-sm-6">
          <div class="card">
            <div class="card-body">
              <h3 class="card-title"><span class="fa fa-user"></span>Local</h3>
              <p>
                <strong>Email</strong> : <%= User.local.Email %> <br>
                <strong>Cont</strong> : <%= User.local.Cont %>
              </p>
              <a href="/Login" class="btn btn-light btn-sm">
                Logout
              </a>
            </div>
          </div>
        </div>
      </div>
    </div>
  </body>
</html>